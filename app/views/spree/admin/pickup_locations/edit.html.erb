<% content_for :page_title do %>
  <%= link_to Spree.t(:pickup_locations), spree.admin_pickup_locations_url %> /
  <%= @pickup_location.name %>
<% end %>

<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @pickup_location } %>

<%= form_for [:admin, @pickup_location] do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/edit_resource_links' %>
  </fieldset>
<% end %>
